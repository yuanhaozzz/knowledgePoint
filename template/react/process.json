{
    "apps": [
        {
            "name": "ipad-h5",
            "script": "./dist/server/server.js",
            "cwd": "./",
            "log_date_format": "YYYY-MM-DD HH:mm Z",
            "error_file": "./logs/app-err.log",
            "out_file": "./logs/app-out.log",
            "instances": 1,
            "min_uptime": "200s",
            "max_restarts": 10,
            "max_memory_restart": "1M",
            "cron_restart": "1 0 * * *",
            "watch": false,
            "merge_logs": true,
            "exec_interpreter": "node",
            "exec_mode": "fork",
            "autorestart": true,
            "vizion": false
        }
    ]
}