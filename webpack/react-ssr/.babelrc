{
    "presets": [
        [
            "@babel/env",
            {
                "targets": {
                    "edge": "17",
                    "firefox": "60",
                    "chrome": "67",
                    "safari": "11.1",
                    "ie": "10"
                },
                // 此参数会使polyfill进行打包
                "useBuiltIns": "usage",
                // 需要指定corejs 的版本
                "corejs": "3.1.4"
            },
        ],
        "@babel/preset-react"
    ],
    "plugins": [
        // 该插件主要减少代码打包体积，避免重复引入
        "@babel/plugin-transform-runtime",
        "@babel/plugin-proposal-class-properties"
    ]
}